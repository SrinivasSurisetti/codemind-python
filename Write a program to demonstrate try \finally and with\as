try:
  a=int(input("Enter 'a' value:"))
  b=int(input("Enter 'b' value:"))
  c=a//b
except ZeroDivisionError:
  print("Division can't possible (b=0)")
else:
finally:
  print(" a//b Value:",c)
  print("End of the program")
  file=open('file1.txt','w')
